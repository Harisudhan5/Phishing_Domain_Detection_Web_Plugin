{
    "manifest_version": 3,
    "name": "URL Monitor",
    "version": "1.0",
    "description": "",
    "permissions": ["activeTab", "tabs"],
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  